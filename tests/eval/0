Y-comb
(let
    (
        ; Thank you, Wikipedia! :-)
        (Y (lambda (f)
            ((lambda (i) (i i))
                (lambda (i)
                    (f (lambda (x) ((i i) x)))))))
        
        (fact (lambda (fact) (lambda (n) (cond
            ((eq? 1 n) 1)
            (#t (* n (fact (- n 1)))) 
        ))))
        (fact (Y fact))
    )
    (fact 5)
)
120
